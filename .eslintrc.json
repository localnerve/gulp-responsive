{
  "root": true,
  "env": {
    "node": true,
    "es2023": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2023
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "indent": [2, 2, {
      "SwitchCase": 1,
      "MemberExpression": 1
    }],
    "quotes": [2, "single"]
  }
}